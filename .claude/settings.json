{
  "permissions": {
    "allow": [
      "Bash(npm install)",
      "Bash(npm run build)",
      "Bash(npm start)",
      "Bash(npm test)",
      "Bash(python main.py)",
      "Bash(pip install:*)",
      "Bash(git status)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Read(README.md)",
      "Read(DEPLOYMENT.md)",
      "Read(package.json)",
      "Read(requirements.txt)"
    ],
    "deny": [
      "Read(.env)",
      "Read(.env.*)",
      "Read(**/*secret*)",
      "Read(**/*password*)",
      "Bash(rm -rf:*)",
      "Bash(sudo:*)"
    ]
  }
}
